@{
    ViewData["Title"] = "News Page";
    Layout = "~/Views/Shared/_UserLayoutdst.cshtml";
}
<section class="page-banner-wrap bg-cover" style="background-image: url(@Href("~/Users/img/page-banner.jpg"))">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-md-12 col-12">
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="@Url.Content("/Index")">Home</a></li>
                        <li class="breadcrumb-item active" aria-current="page">All News Details</li>
                    </ol>
                </nav>
            </div>
        </div>
    </div>
</section>


<div class="about-content common-desc mt-20 mb-50">
    <div class="container-fluid">   
        <div class="row">

            <div class="col-lg-12 col-md-12 col-sm-12 col-12">
                <div class="tab-data">
                    <table class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>
                                    Sr.No
                                </th>
                                <th>
                                    Important Link
                                </th>
                                <th>
                                    Date
                                </th>
                                <th>
                                    Download
                                </th>
                            </tr>
                        </thead>
                        <tbody id="tbodyDV">
                        </tbody>
                    </table>
                </div>
            </div>

        </div>
    </div>
</div>

<!-- End Blog Details -->
@section Scripts {
<script src="@Href("~/ui/js/News.js?dt="+DateTime.Now.ToString("ddMMyyyyfffttfff"))"></script>
}
